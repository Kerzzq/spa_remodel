
{
  "routes": [
  {
  "route": "/data/*",
  "allowedRoles": ["anonymous"]
  },
  {
    "route": "/statics/*",
    "allowedRoles": ["anonymous"]
  },
  {
    "route": "/*",
    "allowedRoles": ["authenticated"]
  }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/data/*", "/statics/*", "/*.css", "/*.js", "/*.ico", "/*.png", "/*.jpg", "/*.jpeg", "/*.svg", "/*.mp4"]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad",
      "statusCode": 302
    }
  }
}
